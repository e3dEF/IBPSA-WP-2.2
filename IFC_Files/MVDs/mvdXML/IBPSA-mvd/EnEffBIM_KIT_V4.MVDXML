        <ConceptRoot uuid="096e4868-149d-438a-9f61-8851c51475f2" name="IfcProject" status="sample" applicableRootEntity="IfcProject">
          <Concepts>
            <Concept uuid="7140d63a-fbda-444b-9e41-d0ff681deb13" name="Project Units" status="sample" override="false">
