              </TemplateRules>
            </Concept>
          </Concepts>
        </ConceptRoot>
        <ConceptRoot uuid="41cb637f-ca53-4e6d-906d-9689f5a20ddf" name="IfcBuildingStorey" status="sample" applicableRootEntity="IfcBuildingStorey">
          <Concepts>
            <Concept uuid="554feb94-4d09-4fa1-a1fd-6b6d5b95ce32" name="Storey Attributes" status="sample" override="false">
